p2_prefix_when_specified=
p2_fileFormat=json
p2_MODE=IN
p2_value=
p2_description=JSON file containing spot/cell and gene statistics for each sample, coming from the spatialGE.Preprocessing module.
p2_optional=
p2_name=sample.info
p2_default_value=
p2_type=java.io.File

